{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "ordered-imports": [
    true,
    {
      "import-sources-order": "lowercase-last",
      "named-imports-order": "lowercase-first"
    }
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}